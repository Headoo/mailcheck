{
    "name": "kcassam/mailcheck",
    "type": "symfony-bundle",
    "description": "eMail checker and suggests a right domain",
    "homepage": "https://github.com/Headoo/mailcheck",
    "keywords": ["mailcheck", "api", "bundle", "domain"],
    "authors": [{
        "name": "Headoo"
       }
    ],
    "require": {
        "php": ">=7.1",
        "ext-intl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {"Mailcheck\\": "src/Mailcheck"}
    },
    "autoload-dev": {
        "psr-4": {"Mailcheck\\Tests\\": "tests"}
    }
}
